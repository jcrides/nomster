<h1>Welcome to Nomster</h1>
<% @places.each do |place| %>
  <div class="nom-box col-xs-10 col-xs-offset-1">
    <h2><%= place.name %></h2>
    <i><%= place.address %></i>
    <p><%= place.description %></p>
    <% if place.user.present? %>
      <small><%= place.user.email %></small>
    <% end %>
  </div>
<% end %>
<%= will_paginate @places %>
